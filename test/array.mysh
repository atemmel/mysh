#!../build/mysh

var numbers = [1 2 3 4]

print numbers is $numbers

$numbers = (append $numbers 5)

print numbers is $numbers

$numbers = $numbers | append 6

print numbers is $numbers

fn isEven x {
	return $x % 2 == 0
}

$numbers | filter isEven

for i in $numbers {
	print i is $i
}

print (len $numbers)
