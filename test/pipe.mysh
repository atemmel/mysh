#!../build/mysh

# classic pipe
#ls | wc -w

fn add5 x {
	return $x + 5
}

var result1 = 5 | add5

# 10
print result1 is $result1

fn ret5 {
	return 5
}

var result2 = (ret5) | add5

# 10
print result2 is $result2

var result3 = (add5 5) | add5

# 15
print result3 is $result3

var result4 = 5 | add5 | add5

# 15
print result4 is $result4

## ux version

fn f x {
	return $x + 5
}

var r1 = 5 | f

print r1 is $r1

fn g {
	return 5
}

var r2 = (g) | f

print r2 is $r2

var r3 = (f 5) | f

print r3 is $r3

var r4 = 5 | f | f

print r4 is $r4
