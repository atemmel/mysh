#!../zig-out/bin/mysh

# shell-like interpolation

var name = "James"
var surname = "Bond"

var fullname = "$name $surname"

print $fullname

var a = "tic"
var b = "tac"
var c = "toe"

print "$a$b$c"

# Fully escape shell interpolation
var escapeTest = "That'll be 5\$, thanks"
print $escapeTest

# Early escape shell interpolation
var type = "Camel"
print "$type\ case"
print "$type case"

# Brace interpolation
print "{type}case"
print "{type} case"
# Escape brace interpolation
print "\{{type}\} case"
